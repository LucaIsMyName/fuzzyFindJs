{
  "name": "fuzzyfindjs",
  "version": "1.0.37",
  "description": "A multi-language optimized fuzzy search library with phonetic matching, compound word splitting, and synonym support",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "unpkg": "./dist/umd/fuzzyfindjs.min.js",
  "jsdelivr": "./dist/umd/fuzzyfindjs.min.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./languages/*": {
      "types": "./dist/types/languages/*.d.ts",
      "import": "./dist/esm/languages/*.js",
      "require": "./dist/cjs/languages/*.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:types && npm run build:esm && npm run build:cjs && npm run build:umd",
    "build:types": "tsc --project tsconfig.build.json",
    "build:esm": "vite build --config vite.config.esm.ts",
    "build:cjs": "vite build --config vite.config.cjs.ts",
    "build:umd": "vite build --config vite.config.umd.ts",
    "verify": "node scripts/verify-build.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:json": "vitest --run --reporter=json > test-results.json",
    "test:coverage": "vitest --coverage",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fuzzy",
    "search",
    "german",
    "english",
    "spanish",
    "french",
    "phonetic",
    "typescript",
    "multilingual"
  ],
  "author": "Luca Mack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LucaIsMyName/fuzzyfindjs.git"
  },
  "bugs": {
    "url": "https://github.com/LucaIsMyName/fuzzyfindjs/issues"
  },
  "homepage": "https://github.com/LucaIsMyName/fuzzyfindjs#readme",
  "devDependencies": {
    "@types/node": "^22.18.13",
    "@vitest/ui": "^2.1.9",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^2.1.9"
  }
}
